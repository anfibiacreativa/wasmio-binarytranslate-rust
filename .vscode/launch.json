    "version": "0.2.0",
    "configurations": [
      
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://[::1]:5173",
            "webRoot": "${workspaceFolder}",
            "trace": true,
            "sourceMaps": true,
            "outFiles": ["${workspaceFolder}/dist/assets/**/*.js", "${workspaceFolder}/dist/assets/**/*.js.map"]
        }
    ]
}